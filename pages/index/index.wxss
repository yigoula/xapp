.demoimage{
    width:149rpx;
    height:149rpx;
    margin:60rpx;
    border-radius:50%;
}
.demousername{
    color:aqua;
    font-size:40rpx;
    margin-top:30rpx;
    font-weight:bold;
}
.demowelcome{
    color:blueviolet;
    font-size: 40rpx;
    margin-top:400rpx;
    font-weight: bold;
}
.container{
    flex-direction:column;
    display: flex;
    align-items: center;
}

.shoppinglist{
    text-align:center;
    width:400px; 
    height:80px;
}
.shoppinglist-ul{
    text-align:center;
    width:400px;
}
.shoppinglist-li{
    text-align:center;
    width:190px;
    height:170px;
    float:left;
    border-right: 1px solid #dcdcdc;
    border-bottom: 1px solid #dcdcdc;
}
.good_title {
	font-size: 12px;
	font-family: 微软雅黑;
}
.progress {
	padding: 10px;
}
.good_progress {
    background: #e7e7e7;
    position: relative;
    border-radius: 3px;
    box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.1);
    overflow: hidden;    
    margin: 10px;
}